<table>
  <thead>
    <tr>Song Id</tr>
    <tr>Song Name</tr>
    <tr>Artist Name</tr>
    <tr>Album Name</tr>
  </thead>
<tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.id %></td>
        <td><%= song.name %></td>
        <td><%= song.cd.artist %></td>
        <td><%= song.cd.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>
